{
  "name": "rn-template",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "build:web": "expo export --platform web",
    "lint": "biome check --fix src --reporter summary",
    "postinstall": "lefthook install",
    "update": "nu && nu expo@latest && expo install --fix",
    "test": "jest",
    "check": "expo-doctor",
    "commit": "git add -A && git-cz"
  },
  "dependencies": {
    "expo": "^52.0.7",
    "expo-constants": "~17.0.3",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.6",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@biomejs/biome": "^1.9.4",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "expo-doctor": "^1.12.4",
    "git-cz": "^4.9.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "lefthook": "^1.8.4",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
